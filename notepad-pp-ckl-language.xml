            <WordsStyle name="LINE COMMENTS" fgColor="008000" bgColor="000000" fontName="0" fontStyle="0" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="C0C0C0" bgColor="000000" fontName="no" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="808080" bgColor="000000" fontName="0" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="C0C0C0" bgColor="000000" fontName="7000" fontStyle="1" nesting="0" />
